# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to git

# Skydio API Configuration
SKYDIO_API_KEY=your_skydio_api_key_here
SKYDIO_API_SECRET=your_skydio_api_secret_here
SKYDIO_BASE_URL=https://cloud.skydio.com/api/v1
SKYDIO_DRONE_ID=your_drone_serial_or_id_here

# CalTopo API Configuration
CALTOPO_CREDENTIAL_ID=your_caltopo_credential_id_here
CALTOPO_CREDENTIAL_SECRET=your_caltopo_credential_secret_here
CALTOPO_BASE_URL=https://caltopo.com
CALTOPO_TEAM_ID=your_team_id_here
CALTOPO_MAP_ID=your_map_id_here

# Google Cloud Configuration
GCP_PROJECT_ID=your_gcp_project_id_here
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Application Configuration
LOG_LEVEL=INFO
UPDATE_INTERVAL_SECONDS=30
TRACK_NAME_PREFIX=SAR-Drone
MAX_TRACK_POINTS=1000

# Development settings
FLASK_ENV=development
DEBUG=True
